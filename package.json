{
  "name": "ghosts-services",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts}' --fix && terraform fmt -recursive infrastructure",
    "up": "docker-compose up -d",
    "start": "serverless offline",
    "provision": "cd infrastructure/local && terraform apply --auto-approve",
    "stop": "docker-compose stop",
    "down": "docker-compose down",
    "deploy:personal": "serverless deploy -s $(whoami) -v --aws-s3-accelerate",
    "destroy:personal": "serverless remove -s $(whoami) -v",
    "deploy:personal-complete": "cd infrastructure/personal && terraform apply --auto-approve && cd ../.. && serverless deploy -s $(whoami) -v --aws-s3-accelerate",
    "destroy:personal-complete": "serverless remove -s $(whoami) -v && cd infrastructure/personal && terraform destroy --auto-approve"
  },
  "dependencies": {
    "@types/elasticsearch": "^5.0.36",
    "@types/http-aws-es": "^6.0.0",
    "@types/sanitize-html": "^1.23.3",
    "@types/shortid": "^0.0.29",
    "aws-sdk": "^2.705.0",
    "axios": "^0.21.1",
    "elasticsearch": "^16.7.1",
    "http-aws-es": "^6.0.0",
    "sanitize-html": "^2.3.2",
    "serverless-api-gateway-throttling": "^1.0.1",
    "serverless-offline": "^6.4.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "shortid": "^2.2.15",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "serverless-domain-manager": "^4.1.1",
    "serverless-localstack": "^0.4.24",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^26.1.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
